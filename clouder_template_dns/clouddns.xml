<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


        <record id="application_type_clouddns" model="clouder.application.type">
            <field name="name">clouddns</field>
            <field name="system_user">clouddns</field>
            <field name="tag_ids" eval="[(6, 0, [ref('clouder.tag_dns')])]"/>
        </record>

        <record id="application_clouddns" model="clouder.application">
            <field name="name">Cloud DNS</field>
            <field name="code">clouddns</field>
            <field name="type_id" ref="application_type_clouddns"/>
            <field name="tag_ids" eval="[(4, [ref('clouder.tag_dns')])]"/>
            <field name="dummy" eval="True"/>
        </record>

    </data>
</odoo>
